<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../../static/head %>

</head>

<body class="page-header-fixed page-sidebar-closed-hide-logo page-content-white page-lines-list">


<div class="page-wrapper">

    <header id="header" valign="botoom">
        <% include ../../static/header %>
    </header>

    <div class="page-container">


        <div class="page-content-wrapper">
            <div class="wrapper-title">
                <h1 class="page-title text-center"> Parties</h1>
            </div>

            <div class="page-content">

                <div class="row">

                    <div class="col-md-12">
                        <div class="pull-left group-btn">
                            <div class="pull-left">
                                <a data-status="false" class="filter-status btn btn-circle btn-success"> All</a>
                                <span>999</span>
                            </div>
                            <div class="pull-left">
                                <a data-status="true" class="filter-status btn btn-circle btn-success"> Past</a>
                                <span>999</span>
                            </div>
                            <div class="pull-left">
                                <a data-status="false" class="filter-status btn btn-circle btn-success"> Today</a>
                                <span>999</span>
                            </div>
                            <div class="pull-left">
                                <a data-status="false" class="filter-status btn btn-circle btn-success"> Future</a>
                                <span>999</span>
                            </div>
                            <a class="btn btn-circle btn-info" data-target="#modal-add-line" data-toggle="modal">Add Party
                                <i class="fa fa-plus-circle" aria-hidden="true"></i>
                            </a>
                        </div>


                        <table id="parties_datatable"
                               class="table table-striped table-bordered table-hover table-checkable order-column">
                            <thead>
                            <tr>
                                <th>#</th>
                                <th>Line</th>
                                <th>Country</th>
                                <th>City</th>
                                <th>Mom Event</th>
                                <th>Date</th>
                                <th>Open Time</th>
                                <th>Attendees</th>
                                <th>Video</th>
                                <th>Tkts</th>
                            </tr>
                            </thead>

                        </table>
                    </div>

                </div>


            </div>

        </div>
    </div>


</div>


<footer>
    <% include ../../static/footer %>

    <script src="/js/party/parties.js"></script>
    <script src="/assets/global/scripts/datatable.js" type="text/javascript"></script>
    <script src="/assets/global/plugins/datatables/datatables.min.js" type="text/javascript"></script>
</footer>

<% include ../../static/menu %>

</body>
</html>


